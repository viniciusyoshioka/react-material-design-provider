{
  "name": "react-material-design-provider",
  "description": "Unofficial implementation of Material Design 3 theme provider for React",
  "version": "1.2.0",
  "license": "MIT",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index",
  "react-native": "src/index",
  "files": [
    "src",
    "lib",
    "README.md"
  ],
  "author": "Vinicius Yoshioka <71671043+viniciusyoshioka@users.noreply.github.com> (https://github.com/viniciusyoshioka)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viniciusyoshioka/react-material-design-provider.git"
  },
  "bugs": {
    "url": "https://github.com/viniciusyoshioka/react-material-design-provider/issues"
  },
  "homepage": "https://github.com/viniciusyoshioka/react-material-design-provider#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "material design",
    "material you",
    "theme",
    "react",
    "react-native"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "bootstrap": "yarn && cd example && yarn",
    "example": "yarn --cwd example",
    "start": "yarn --cwd example start",
    "android": "yarn --cwd example android",
    "ios": "yarn --cwd example ios",
    "build": "bob build"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "color": "^4.2.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@types/color": "^3.0.6",
    "@types/react": "^18.2.58",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vinicius1313/eslint-config": "^1.2.2",
    "@vinicius1313/tsconfig": "^1.0.0",
    "eslint": "^8.57.0",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "^5.4.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ]
  }
}
